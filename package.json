{
  "name": "n8n-nodes-mailtrap",
  "version": "1.0.0",
  "description": "n8n node for Mailtrap integration (send email, manage contacts)",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/railsware/mailtrap-n8n"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes --write",
    "lint": "eslint nodes package.json --ext .ts",
    "lintfix": "eslint nodes package.json --fix",
    "test": "jest",
    "prepublishOnly": "npm run build && npm run lint -c .eslintrc.prepublish.js nodes package.json"
  },
  "keywords": [
    "n8n",
    "n8n-node",
    "n8n-community-node-package",
    "mailtrap",
    "email"
  ],
  "author": {
    "name": "temp",
    "email": "temp@temp.com"
  },
  "license": "MIT",
  "dependencies": {
    "n8n-core": "^1.88.0",
    "n8n-workflow": "^1.87.0"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/nodes/v1/credentials/MailtrapTokenApi.credentials.js",
      "dist/nodes/v1/credentials/MailtrapBearerTokenApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/Mailtrap.node.js",
      "dist/nodes/v1/MailtrapV1.node.js"
    ]
  },
  "devDependencies": {
    "@types/eslint": "^9.6.1",
    "@types/jest": "^29.5.0",
    "@types/lodash": "^4.17.16",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "eslint-plugin-n8n-nodes-base": "^1.16.1",
    "gulp": "^5.0.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
